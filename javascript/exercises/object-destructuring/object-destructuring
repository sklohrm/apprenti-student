const book = {
    title: "The Great Gatsby",
    author: "F. Scott Fitzgerald",
    year: 1925,
};
console.log(book.title, book.author, book.year);

// 1. Refactor the code using object destructuring so that you extract title, author, and year in one line.
let { title, author, year } = book;

// 2. Log the destructured values.
console.log(`Title: ${title} | Author: ${author} | Year: ${year}`);